<?xml version="1.0" encoding="utf-8"?>
<resources>

  <!--  CustomTextView自定义属性-->
<declare-styleable name="CustomTextView">

    <attr name="text" format="string"/>

</declare-styleable>


<declare-styleable name="CircleImageView">
    <attr name="circle_border_width" format="dimension"/>
    <attr name="circle_border_color" format="color"/>
</declare-styleable>
    
    
<declare-styleable name="CodeView">
    <!--验证码个数-->
    <attr name="num_count" format="integer"/>
    <!--干挠线条数-->
    <attr name="line_count" format="integer"/>
   <!-- 验证码字体大小-->
    <attr name="font_size" format="dimension"/>
    <!--验证码字体颜色-->
    <attr name="code_color" format="color"/>
</declare-styleable>    
    
    <declare-styleable name="CornerLayout3">
        <attr name="layout_position" format="enum">
            <enum name="left_top" value="0"/>
            <enum name="right_top" value="1"/>
            <enum name="left_bottom" value="2"/>
            <enum name="right_bottom" value="3"/>
        </attr>
    </declare-styleable>
    
    <!--侧边栏属性-->
    <declare-styleable name="SliderMenu">
        <!--侧边栏宽度-->
        <attr name="sliding_width" format="dimension"/>
        <!--分割线宽度-->
        <attr name="divider_width" format="dimension"/>
        <!--手指感应宽度-->
        <attr name="touch_width" format="dimension"/>
    </declare-styleable>
    
</resources>